{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-strongly-recommended",
    "plugin:json/recommended-with-comments",
    "@nuxt/eslint-config",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "vue/multi-word-component-names": 0
  },
  "overrides": [
    {
      "files": ["*.js", "*.ts"],
      "rules": {
        "no-undef": "off"
      }
    }
  ],
  "settings": {}
}
